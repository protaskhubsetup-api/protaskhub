{
  "version": 2,
  "functions": {
    "api/chat.js": {
      "runtime": "nodejs18.x"
    },
    "api/admin.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/api/chat.js"
    },
    {
      "src": "/api/admin/prices",
      "dest": "/api/admin.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}